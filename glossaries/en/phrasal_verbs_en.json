  "separability_rules": {
    "description": "Rules for object placement with separable phrasal verbs",
    "rules": [
      {
        "rule": "With noun objects, particle can go before or after",
        "examples": ["Turn off the light.", "Turn the light off."]
      },
      {
        "rule": "With pronoun objects, particle MUST go after",
        "correct": "Turn it off.",
        "incorrect": "Turn off it.",
        "severity": "critical"
      },
      {
        "rule": "Inseparable phrasal verbs never separate",
        "correct": "Look after the children.",
        "incorrect": "Look the children after."
      }
    ]
  },
  "translation_guidelines": {
    "rules": [
      "NEVER translate phrasal verbs word-by-word",
      "Find equivalent idiom/expression in target language",
      "Consider all possible meanings based on context",
      "Check if target language has similar phrasal construction",
      "When unsure, use explanatory translation"
    ],
    "common_mt_errors": [
      "'take off' → 'взять прочь' instead of 'снять/взлететь'",
      "'give up' → 'дать вверх' instead of 'сдаться'",
      "'look after' → 'смотреть после' instead of 'заботиться'"
    ]
  }
