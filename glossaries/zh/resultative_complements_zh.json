  "degree_complements": {
    "term_zh": "程度补语",
    "description": "Complements indicating degree or extent",
    "structure": "Verb + 得 + Adjective/Description",
    "marker": "得 (de) is required for degree complements",
    "examples": [
      {
        "sentence": "他跑得很快",
        "meaning": "He runs very fast",
        "structure": "跑 + 得 + 很快"
      },
      {
        "sentence": "她唱得非常好",
        "meaning": "She sings very well",
        "structure": "唱 + 得 + 非常好"
      },
      {
        "sentence": "我累得走不动了",
        "meaning": "I'm so tired I can't walk",
        "structure": "累 + 得 + 走不动了"
      },
      {
        "sentence": "他高兴得跳起来",
        "meaning": "He was so happy he jumped up",
        "structure": "高兴 + 得 + 跳起来"
      }
    ]
  },
  "detection_rules": {
    "rule_1": {
      "name": "Missing resultative complement",
      "description": "Flag verbs that typically require resultative complement but are missing",
      "severity": "minor",
      "example": "*我做了作业 → 我做完了作业 (when completion is implied)"
    },
    "rule_2": {
      "name": "Wrong complement choice",
      "description": "Flag incorrect complement for the action type",
      "severity": "major",
      "example": "*打好玻璃 → 打破玻璃"
    },
    "rule_3": {
      "name": "得/的 confusion with complements",
      "description": "Flag using 的 instead of 得 for degree complements",
      "severity": "critical",
      "example": "*跑的很快 → 跑得很快"
    },
    "rule_4": {
      "name": "Potential complement error",
      "description": "Flag incorrect potential complement formation",
      "severity": "major",
      "example": "*看不得见 → 看不见"
    }
  }
}
