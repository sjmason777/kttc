  "potential_complements": {
    "term_zh": "可能补语",
    "description": "Complements expressing possibility using 得/不",
    "structure": {
      "positive": "Verb + 得 + Complement (能做到)",
      "negative": "Verb + 不 + Complement (做不到)"
    },
    "examples": [
      {
        "positive": "看得见",
        "negative": "看不见",
        "meaning": "can/cannot see"
      },
      {
        "positive": "听得懂",
        "negative": "听不懂",
        "meaning": "can/cannot understand"
      },
      {
        "positive": "做得完",
        "negative": "做不完",
        "meaning": "can/cannot finish"
      },
      {
        "positive": "吃得下",
        "negative": "吃不下",
        "meaning": "can/cannot eat (have appetite)"
      },
      {
        "positive": "睡得着",
        "negative": "睡不着",
        "meaning": "can/cannot sleep"
      },
      {
        "positive": "来得及",
        "negative": "来不及",
        "meaning": "can/cannot make it in time"
      },
      {
